                android:textSize="11sp" />
        </TableRow>

        <TableRow android:orientation="vertical">
            // Row 6

            <EditText
                android:id="@+id/profileRowSixMedInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_med_name"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingTop="0dp"
                android:paddingEnd="3dp"
                android:paddingBottom="0dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowSixDosageInput"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#C0C0C0"
                android:gravity="center"
                android:hint="@string/profile_dosage"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingEnd="3dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowSixDurationInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_duration"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingEnd="3dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />
        </TableRow>

        <TableRow android:orientation="vertical">
            // Row 7

            <EditText
                android:id="@+id/profileRowSevenMedInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_med_name"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingTop="0dp"
                android:paddingEnd="3dp"
                android:paddingBottom="0dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowSevenDosageInput"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#C0C0C0"
                android:gravity="center"
                android:hint="@string/profile_dosage"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingEnd="3dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowSevenDurationInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_duration"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingEnd="3dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />
        </TableRow>

        <TableRow android:orientation="vertical">
            // Row 8

            <EditText
                android:id="@+id/profileRowEightMedInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_med_name"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingTop="0dp"
                android:paddingEnd="3dp"
                android:paddingBottom="0dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowEightDosageInput"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#C0C0C0"
                android:gravity="center"
                android:hint="@string/profile_dosage"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingEnd="3dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowEightDurationInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_duration"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingEnd="3dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />
        </TableRow>

        <TableRow android:orientation="vertical">
            // Row 9

            <EditText
                android:id="@+id/profileRowNineMedInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_med_name"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingTop="0dp"
                android:paddingEnd="3dp"
                android:paddingBottom="0dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowNineDosageInput"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="textHints"
                android:background="#C0C0C0"
                android:gravity="center"
                android:hint="@string/profile_dosage"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingEnd="3dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowNineDurationInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
