                android:textSize="11sp" />
        </TableRow>

        <TableRow android:orientation="vertical">
            // Row 4

            <EditText
                android:id="@+id/profileRowFourMedInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_med_name"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingTop="0dp"
                android:paddingEnd="3dp"
                android:paddingBottom="0dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowFourDosageInput"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#C0C0C0"
                android:gravity="center"
                android:hint="@string/profile_dosage"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingEnd="3dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowFourDurationInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_duration"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingEnd="3dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />
        </TableRow>

        <TableRow android:orientation="vertical">
            // Row 5

            <EditText
                android:id="@+id/profileRowFiveMedInput"
                android:layout_width="125dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:autofillHints="testHint"
                android:background="#808080"
                android:gravity="center"
                android:hint="@string/profile_med_name"
                android:inputType="text"
                android:paddingStart="3dp"
                android:paddingTop="0dp"
                android:paddingEnd="3dp"
                android:paddingBottom="0dp"
                android:textColor="@android:color/black"
                android:textSize="11sp" />

            <EditText
                android:id="@+id/profileRowFiveDosageInput"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
